AA,EAAA,EAAA,IAAA,CAAA;AADA,OAAA;AAGA,WAAA,MAAA,CAAA,GAAA,CAAA,0BAAA,EAAA,cAAA,EAAA,IAAA,CAAA,GAAA,IAAA;AACA,YAAA,GAAA,CAAA,IAAA,CAAA,IAAA,KAAA,GAAA,EAAA;AACA,eAAA,OAAA,CAAA;AACA,YAAA,QAAA,EAAA,IADA;AAEA,YAAA,KAAA,EAAA,MAFA;AAGA,YAAA,OAAA,EAAA,MAHA;AAIA,YAAA,IAAA,EAAA;AAJA,WAAA;AAMA,eAAA,cAAA;AACA;AACA,OAVA,EAUA,KAVA,CAUA,KAAA,IAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,OAAA,EAAA,KAAA;AACA,OAZA;AAaA,KAlBA;;AAmBA;AACA,UAAA,WAAA,CAAA,YAAA,EAAA;AACA,YAAA,SAAA,GAAA,MAAA,KAAA,YAAA,CAAA;AACA,QAAA,KAAA,EAAA,YADA;AAEA,QAAA,IAAA,EAAA,eAFA;AAGA,QAAA,IAAA,EAAA;AAHA,OAAA,CAAA;;AAKA,UAAA,SAAA,EAAA;AACA,YAAA;AACA,cAAA,GAAA,GAAA,CAAA,YAAA,CAAA,EAAA,CAAA;AACA,gBAAA,QAAA,GAAA,MAAA,KAAA,MAAA,CAAA,IAAA,CAAA,+BAAA,EAAA,GAAA,CAAA;AACA,gBAAA;AAAA,YAAA;AAAA,cAAA,QAAA;;AACA,cAAA,IAAA,CAAA,IAAA,KAAA,GAAA,EAAA;AACA,iBAAA,OAAA,CAAA;AACA,cAAA,QAAA,EAAA,IADA;AAEA,cAAA,KAAA,EAAA,MAFA;AAGA,cAAA,OAAA,EAAA,MAHA;AAIA,cAAA,IAAA,EAAA;AAJA,aAAA;AAMA,iBAAA,cAAA;AACA;AACA,SAbA,CAaA,OAAA,KAAA,EAAA;AACA,eAAA,OAAA,CAAA;AACA,YAAA,QAAA,EAAA,IADA;AAEA,YAAA,KAAA,EAAA,MAFA;AAGA,YAAA,OAAA,EAAA,KAHA;AAIA,YAAA,IAAA,EAAA;AAJA,WAAA;AAMA,UAAA,OAAA,CAAA,KAAA,CAAA,aAAA,EAAA,KAAA;AACA;AACA;AACA,KAlDA;;AAmDA,UAAA,cAAA,GAAA;AACA,UAAA;AACA,cAAA,MAAA,GAAA;AACA,UAAA,OAAA,EAAA,KAAA,WADA;AAEA,UAAA,IAAA,EAAA,KAAA,QAFA;AAGA,aAAA,KAAA;AAHA,SAAA;AAKA,cAAA,QAAA,GAAA,MAAA,KAAA,MAAA,CAAA,IAAA,CAAA,6BAAA,EAAA,MAAA,CAAA;AACA,cAAA;AAAA,UAAA;AAAA,YAAA,QAAA;AACA,aAAA,SAAA,GAAA,IAAA,CAAA,IAAA;AACA,aAAA,UAAA,GAAA,IAAA,CAAA,KAAA;AACA,OAVA,CAUA,OAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,KAAA,CAAA,eAAA,EAAA,KAAA;AACA;AACA,KAjEA;;AAkEA,IAAA,GAAA,GAAA;AACA,WAAA,eAAA,GAAA,IAAA;AACA,KApEA;;AAqEA,IAAA,YAAA,GAAA;AACA,WAAA,WAAA,GAAA,CAAA;AACA,WAAA,cAAA;AACA,KAxEA;;AAyEA,IAAA,iBAAA,GAAA;AACA,WAAA,YAAA;AACA,KA3EA;;AA4EA,IAAA,gBAAA,CAAA,GAAA,EAAA;AACA,WAAA,QAAA,GAAA,GAAA;AACA,WAAA,WAAA,GAAA,CAAA;AACA,WAAA,cAAA;AACA,KAhFA;;AAiFA,IAAA,mBAAA,CAAA,GAAA,EAAA;AACA,WAAA,WAAA,GAAA,GAAA;AACA,WAAA,cAAA;AACA,KApFA;;AAqFA,IAAA,WAAA,CAAA,GAAA,EAAA;AACA,WAAA,IAAA,GAAA,EAAA,GAAA;AAAA,OAAA;AACA,KAvFA;;AAwFA,IAAA,UAAA,CAAA,GAAA,EAAA;AACA,WAAA,eAAA,GAAA,IAAA;AACA,WAAA,IAAA,GAAA,EAAA,GAAA;AAAA,OAAA;AACA,KA3FA;;AA4FA,IAAA,YAAA,CAAA,GAAA,EAAA;AACA,WAAA,WAAA,CAAA,GAAA;AACA;;AA9FA;AAxBA,CAAA","sourcesContent":["<template>\r\n    <el-row style=\"margin-top: 20px;\">\r\n        <!-- 条件搜索 -->\r\n        <div class=\"word-search\">\r\n            <div class=\"item\">\r\n                <input type=\"text\" placeholder=\"书籍ID\" v-model=\"bookQueryDto.bookId\">\r\n                <i class=\"el-icon-search\" @click=\"fetchFreshData\"></i>\r\n            </div>\r\n        </div>\r\n        <el-row style=\"margin: 10px 0;\">\r\n            <el-row v-if=\"tableData.length === 0\">\r\n                <el-empty description=\"暂无书籍借阅历史\"></el-empty>\r\n            </el-row>\r\n            <el-row v-else>\r\n                <div style=\"margin: 0 auto;width: 1200px;\">\r\n                    <el-col v-for=\"(book, index) in tableData\" :key=\"index\" :span=\"6\">\r\n                        <div class=\"item-book\">\r\n                            <div style=\"display: flex;justify-content: center;\">\r\n                                <img style=\"width: 100%;max-height: 380px;border-radius: 5px;\" :src=\"book.bookCover\"\r\n                                    alt=\"\">\r\n                            </div>\r\n                            <div style=\"padding: 10px 22px;\">\r\n                                <div style=\"color: rgb(51,51,51);font-size: 24px;font-weight: bold;margin-block: 4px;\">\r\n                                    <el-tooltip class=\"item\" effect=\"dark\" :content=\"book.bookName\"\r\n                                        placement=\"bottom-end\">\r\n                                        <div class=\"title\">{{ book.bookName }}</div>\r\n                                    </el-tooltip>\r\n                                </div>\r\n                                <div class=\"title\" style=\"margin-block: 8px;\">\r\n                                    <el-tooltip v-if=\"!book.isReturn\" class=\"item\" effect=\"dark\" content=\"请及时归还\"\r\n                                        placement=\"bottom-end\">\r\n                                        <span\r\n                                            style=\"font-size: 14px;text-decoration: underline;text-decoration-style: dashed;\">待归还</span>\r\n                                    </el-tooltip>\r\n                                    <span v-else style=\"font-size: 14px;\">已归还</span>\r\n                                </div>\r\n                                <div style=\"margin-block: 6px;font-size: 12px;color: rgb(51,51,51);margin-block: 4px;\">\r\n                                    <div style=\"font-size: 12px;margin-top: 6px;\">\r\n                                        <div class=\"title\">借阅【{{ book.deadlineNum }}】本</div>\r\n                                    </div>\r\n                                    <div>\r\n                                        <el-tooltip class=\"item\" effect=\"dark\" :content=\"book.createTime\"\r\n                                            placement=\"bottom-end\">\r\n                                            <div class=\"title\" style=\"margin-block: 10px;\">\r\n                                                <span style=\"margin-right: 5px;\">借阅时间：{{ book.createTime }}</span>\r\n                                            </div>\r\n                                        </el-tooltip>\r\n\r\n                                        <div class=\"title\" style=\"margin-block: 10px;\">\r\n                                            <span style=\"margin-right: 5px;\">归还时间：{{ book.returnTime }}</span>\r\n                                        </div>\r\n                                    </div>\r\n                                </div>\r\n                                <div>\r\n                                    <span class=\"edit-round\" v-if=\"book.isReturn === false\"\r\n                                        @click=\"handleReturn(book)\">还书</span>\r\n                                    <span class=\"edit-round\" v-else @click=\"handleDelete(book)\">删除</span>\r\n                                </div>\r\n                            </div>\r\n                        </div>\r\n                    </el-col>\r\n                </div>\r\n\r\n            </el-row>\r\n        </el-row>\r\n        <div class=\"pager\" v-if=\"tableData.length !== 0\">\r\n            <div>\r\n                <el-pagination @size-change=\"handleSizeChange\" @current-change=\"handleCurrentChange\"\r\n                    :current-page.sync=\"current\" :page-size=\"size\" layout=\"total, prev, pager, next\"\r\n                    :total=\"totalCount\">\r\n                </el-pagination>\r\n            </div>\r\n        </div>\r\n    </el-row>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n    data() {\r\n        return {\r\n            deadlineNum: 1,\r\n            data: {},\r\n            current: 1,\r\n            cover: null,\r\n            size: 10,\r\n            totalItems: 0,\r\n            dialogOperation: false, // 开关\r\n            isOperation: false, // 开关-标识新增或修改\r\n            tableData: [],\r\n            selectedRows: [],\r\n            bookQueryDto: {}, // 搜索条件\r\n            options: [],\r\n            bookshelfOptions: [],\r\n            categoryItemSelected: null,\r\n            categoryList: [],\r\n            bookOrderHistorys: []\r\n        };\r\n    },\r\n    created() {\r\n        this.fetchFreshData();\r\n    },\r\n    methods: {\r\n        handleReturn(book) {\r\n            const bookRssHistory = {\r\n                id: book.id\r\n            }\r\n            this.$axios.put('/bookOrderHistory/update', bookRssHistory).then(res => {\r\n                if (res.data.code === 200) {\r\n                    this.$notify({\r\n                        duration: 1000,\r\n                        title: '还书操作',\r\n                        message: '还书成功',\r\n                        type: 'success'\r\n                    });\r\n                    this.fetchFreshData();\r\n                }\r\n            }).catch(error => {\r\n                console.log(\"还书异常：\", error);\r\n            })\r\n        },\r\n        // 批量删除数据\r\n        async batchDelete(orderHistory) {\r\n            const confirmed = await this.$swalConfirm({\r\n                title: '删除书籍预约历史数据',\r\n                text: `删除后不可恢复，是否继续？`,\r\n                icon: 'warning',\r\n            });\r\n            if (confirmed) {\r\n                try {\r\n                    let ids = [orderHistory.id];\r\n                    const response = await this.$axios.post(`/bookOrderHistory/batchDelete`, ids);\r\n                    const { data } = response;\r\n                    if (data.code === 200) {\r\n                        this.$notify({\r\n                            duration: 1000,\r\n                            title: '删除操作',\r\n                            message: '删除成功',\r\n                            type: 'success'\r\n                        });\r\n                        this.fetchFreshData();\r\n                    }\r\n                } catch (error) {\r\n                    this.$notify({\r\n                        duration: 2000,\r\n                        title: '删除失败',\r\n                        message: error,\r\n                        type: 'error'\r\n                    });\r\n                    console.error(`书籍预约信息删除异常：`, error);\r\n                }\r\n            }\r\n        },\r\n        async fetchFreshData() {\r\n            try {\r\n                const params = {\r\n                    current: this.currentPage,\r\n                    size: this.pageSize,\r\n                    ...this.bookQueryDto\r\n                };\r\n                const response = await this.$axios.post('/bookOrderHistory/queryUser', params);\r\n                const { data } = response;\r\n                this.tableData = data.data;\r\n                this.totalCount = data.total;\r\n            } catch (error) {\r\n                console.error('查询书籍借阅历史信息异常:', error);\r\n            }\r\n        },\r\n        add() {\r\n            this.dialogOperation = true;\r\n        },\r\n        handleFilter() {\r\n            this.currentPage = 1;\r\n            this.fetchFreshData();\r\n        },\r\n        handleFilterClear() {\r\n            this.handleFilter();\r\n        },\r\n        handleSizeChange(val) {\r\n            this.pageSize = val;\r\n            this.currentPage = 1;\r\n            this.fetchFreshData();\r\n        },\r\n        handleCurrentChange(val) {\r\n            this.currentPage = val;\r\n            this.fetchFreshData();\r\n        },\r\n        messagePush(row) {\r\n            this.data = { ...row };\r\n        },\r\n        handleEdit(row) {\r\n            this.dialogOperation = true;\r\n            this.data = { ...row };\r\n        },\r\n        handleDelete(row) {\r\n            this.batchDelete(row);\r\n        }\r\n    },\r\n};\r\n</script>\r\n<style scoped lang=\"scss\">\r\n.item-book {\r\n    border: 1px solid rgb(235, 235, 235);\r\n    margin: 5px;\r\n    border-radius: 5px;\r\n    padding: 6px;\r\n    box-sizing: border-box;\r\n}\r\n\r\n.title {\r\n    width: 140px;\r\n    overflow: hidden;\r\n    /* 隐藏超出部分 */\r\n    text-overflow: ellipsis;\r\n    /* 使用省略号表示超出部分 */\r\n    white-space: nowrap;\r\n    /* 禁止换行 */\r\n}\r\n\r\n.pager {\r\n    display: flex;\r\n    margin-block: 20px;\r\n    justify-content: center;\r\n    align-items: center;\r\n}\r\n\r\n.category {\r\n    font-size: 16px;\r\n    color: rgb(114, 114, 114);\r\n    cursor: pointer;\r\n    display: inline-block;\r\n    transition: all .2s;\r\n    padding: 10px 20px;\r\n    border-radius: 10px;\r\n}\r\n\r\n.word-search {\r\n    display: flex;\r\n    justify-content: center;\r\n\r\n    .item {\r\n        padding: 14px;\r\n        width: 500px;\r\n        background-color: rgb(247, 247, 247);\r\n        border-radius: 10px;\r\n        display: flex;\r\n        justify-content: space-between;\r\n        align-items: center;\r\n        box-sizing: border-box;\r\n\r\n        input {\r\n            border: none;\r\n            background-color: rgb(247, 247, 247);\r\n            outline: none;\r\n            font-size: 16px;\r\n        }\r\n\r\n        i {\r\n            padding: 6px;\r\n            border-radius: 5px;\r\n            cursor: pointer;\r\n        }\r\n\r\n        i:hover {\r\n            background-color: rgb(241, 241, 241);\r\n        }\r\n    }\r\n\r\n}\r\n</style>"],"sourceRoot":"src/views/user"}]}